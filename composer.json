{
    "name": "narokishi/number-to-words",
    "description": "PHP package to transform numbers into words",
    "keywords": ["php", "number", "words", "numbertowords", "wordsfromnumber"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomasz Motyl",
            "email": "tomasz.motyl806@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "Narokishi\\NumberToWords\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Narokishi\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^7.3.2"
    },
    "scripts": {
        "tests": "./vendor/phpunit/phpunit/phpunit",
        "tests-windows": "php vendor/phpunit/phpunit/phpunit"
    }
}
